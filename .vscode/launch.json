   "version": "0.2.0",
   "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "program": "./CoreEngineCompiler.exe",
            "args": ["../../TestData/TestProject.ceproj"],
            "cwd": "${workspaceFolder}/build/Windows",
            "console": "internalConsole",
            "stopAtEntry": false,
            "osx": {
                "cwd": "${workspaceFolder}/build/MacOS",
                "program": "./CoreEngineCompiler"
            },
            "windows": {
                "cwd": "${workspaceFolder}/build/Windows/",
                "program": "./CoreEngineCompiler.exe"
            }
        }
    ]
}