  - Entity: 'MainCamera'
    Components:
      - Component: 'CoreEngine.Graphics.Components.CameraComponent'
        IsActive: true
      - Component: 'CoreEngine.Components.TransformComponent'
        Position: [-100, 100, 0]
        RotationY: 90
      - Component: 'CoreEngine.Tests.EcsTest.PlayerComponent'
        MovementAcceleration: 30000
  - Entity: 'TopCamera'
    Components:
      - Component: 'CoreEngine.Graphics.Components.CameraComponent'
      - Component: 'CoreEngine.Components.TransformComponent'
        Position: [-100, 1000, 0]
        RotationY: 90
        RotationX: 90
      - Component: 'CoreEngine.Tests.EcsTest.PlayerComponent'
        MovementAcceleration: 30000