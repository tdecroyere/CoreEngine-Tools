Entities:
  - Entity: 'Scene'
    Components:
      - Component: 'CoreEngine.Components.SceneComponent'
        ActiveCamera: 'MainCamera'
  - Entity: 'Teapot1'
    Components:
      - Component: 'CoreEngine.Components.TransformComponent'
        Position: [0, 100, 0]
        RotationY: 90
      - Component: 'CoreEngine.Graphics.Components.MeshComponent'
        MeshResourceId: 'resource:/teapot.mesh'
  - Entity: 'Teapot2'
    Components:
      - Component: 'CoreEngine.Components.TransformComponent'
        Position: [50, 100, 0]
        RotationY: 45
      - Component: 'CoreEngine.Graphics.Components.MeshComponent'
        MeshResourceId: 'resource:/teapot.mesh'
  - Entity: 'TestEntity2'
    Components:
      - Component: 'CoreEngine.Graphics.Components.MeshComponent'
        MeshResourceId: 'resource:/sponza.mesh'
      - Component: 'CoreEngine.Components.TransformComponent'
